<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hy.mapper.tablet.TabletMapper">


	<!-- selectAll: tablet 테이블의 전체 데이터 조회 쿼리 -->
  <select id="selectAll" resultType="com.hy.dto.tablet.Tablet">
    SELECT tablet_id as tabletId,
    tablet_available as tabletAvailable FROM tablet
  </select>

</mapper>